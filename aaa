SELECT
IIf(
(存货名称 like '*水溶液*' or 规格型号 like '*水溶液*' or 规格型号 like '*aqueous solution*' or 英文名称 like '*aqueous solution*'),CAS号&'[水溶液]',
IIf(
(存货名称 like '*甲醇溶液*' or 规格型号 like '*甲醇溶液*' or 规格型号 like '*in Methanol*' or 英文名称 like '*in Methanol*'),CAS号&'[甲醇溶液]',
IIf(
(存货名称 like '*乙醇溶液*' or 规格型号 like '*乙醇溶液*'or 规格型号 like '*in Ethanol*' or 英文名称 like '*in Ethanol*'),CAS号&'[乙醇溶液]',
IIf(
(((存货名称 like'*醇溶液*' and 存货名称 not like '*[甲乙]醇溶液*') or (规格型号 like '*醇溶液*' and 规格型号 not like '*[甲乙]醇溶液*'))
or 规格型号 like '*in Alcohol*' or 英文名称 like '*in Alcohol*'),CAS号&'[醇溶液]',
IIf(
(存货名称 like'*丙酮溶液*' or 规格型号 like'*丙酮溶液*' or 规格型号 like '*in Acetone*' or 英文名称 like '*in Acetone*') ,CAS号&'[丙酮溶液]',
IIf(
(存货名称 like'*甲苯溶液*' or 规格型号 like'*甲苯溶液*' or 规格型号 like '*in Toluene*' or 英文名称 like '*in Toluene*'),CAS号&'[甲苯溶液]',
IIf(
(存货名称 like'*四氯化碳溶液*' or 规格型号 like'*四氯化碳溶液*' or 规格型号 like '*in Carbon tetrachloride*' or 英文名称 like '*in Carbon tetrachloride*'),CAS号&'[四氯化碳溶液]',
IIf(
(存货名称 like'*乙醚溶液*' or 规格型号 like'*乙醚溶液*' or 规格型号 like '*in Ether*' and 英文名称 like '*in Ether*'),CAS号&'[乙醚溶液]',
IIf(
(存货名称 like'*乙酸溶液*' or 规格型号 like'*乙酸溶液*' or 规格型号 like '*in Acetic acid*' or 英文名称 like '*in Acetic acid*')and (CAS号 <>null and CAS号<>'NA'),CAS号&'[乙酸溶液]',
IIf(
(存货名称 like'*稀硫酸溶液*' or 规格型号 like'*稀硫酸溶液*' or 规格型号 like '*in dilute Sulfuric acid*' or 英文名称 like '*in dilute Sulfuric acid*'),CAS号&'[稀硫酸溶液]',
IIf(
((存货名称 like'*硫酸溶液*' and 存货名称 not like '*稀硫酸溶液*' or 规格型号 like '*硫酸溶液*') or 规格型号 like '*in Sulfuric acid*'
and 规格型号 not like '*稀硫酸溶液*'or 英文名称 like '*in Sulfuric acid*'),CAS号&'[硫酸溶液]','NA'))))))))))) AS CAS索引号, * into aaa FROM noNA
where
(存货名称 like '*水溶液*'
or 存货名称 like '*甲醇溶液*'
or 存货名称 like '*乙醇溶液*'
or (存货名称 like '*醇溶液*' and 存货名称 like '*甲醇溶液*'and 存货名称 like '*乙醇溶液*')
or 存货名称 like '*丙酮溶液*'
or 存货名称 like '*甲苯溶液*'
or 存货名称 like '*四氯化碳溶液*'
or 存货名称 like '*乙醚溶液*'
or 存货名称 like '*乙酸溶液*'
or 存货名称 like '*稀硫酸溶液*'
or (存货名称 like '*硫酸溶液*' and 存货名称 not like '*稀硫酸溶液*')
or 规格型号 like '*水溶液*'
or 规格型号 like '*甲醇溶液*'
or 规格型号 like '*乙醇溶液*'
or (规格型号 like '*醇溶液*' and 规格型号 not like '*甲醇溶液*' and 规格型号 not like '*乙醇溶液*')
or 规格型号 like '*丙酮溶液*'
or 规格型号 like '*甲苯溶液*'
or 规格型号 like '*四氯化碳溶液*'
or 规格型号 like '*乙醚溶液*'
or 规格型号 like '*乙酸溶液*'
or 规格型号 like '*稀硫酸溶液*'
or (规格型号 like '*硫酸溶液*' and 规格型号 not like '*稀硫酸溶液*')
or 规格型号 like '*aqueous solution*'
or 规格型号 like '*in Methanol*'
or 规格型号 like '*in Ethanol*'
or 规格型号 like '*in Ethanol*'
or 规格型号 like '*in Acetone*'
or 规格型号 like '*in Toluene*'
or 规格型号 like '*in Carbon tetrachloride*'
or 规格型号 like '*in Ether*'
or 规格型号 like '*in Acetic acid*'
or 规格型号 like '*in dilute Sulfuric acid*'
or 规格型号 like '*in Acetone*'
or 规格型号 like '*in Sulfuric acid*'
or 英文名称 like '*aqueous solution*'
or 英文名称 like '*in Methanol*'
or 英文名称 like '*in Ethanol*'
or 英文名称 like '*in Ethanol*'
or 英文名称 like '*in Acetone*'
or 英文名称 like '*in Toluene*'
or 英文名称 like '*in Carbon tetrachloride*'
or 英文名称 like '*in Ether*'
or 英文名称 like '*in Acetic acid*'
or 英文名称 like '*in dilute Sulfuric acid*'
or 英文名称 like '*in Acetone*'
or 英文名称 like '*in Sulfuric acid*')

14









