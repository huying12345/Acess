SELECT *
FROM (SELECT 部门,name as 姓名 FROM (SELECT name  FROM 试剂网后台 WHERE name not like '*实习*' and name not like '*见习*' and name not like '*培训*' and name not like '*审核*' and name not like '*测试*' and name in(select 姓名 from 员工通讯录))  AS table1 LEFT JOIN 员工通讯录 ON table1.name=员工通讯录.姓名) AS [%$##@_Alias]
WHERE 姓名 not in(select 姓名 from 离职人员名单) and 部门 not like '*信息部*' and 部门 like '*苏州公司*';