SELECT
IIf(
((存货名称 like '*盐酸溶液*' and 存货名称 not like '*稀盐酸溶液*' )
 or (规格型号 like '*盐酸溶液*' and 规格型号 not like '*稀盐酸溶液*') or 规格型号 like '*in Hydrochloric acid*' or 英文名称 like '*in Hydrochloric acid*')
,'[盐酸溶液]',
IIf(
(存货名称 like '*乙腈溶液*' or 规格型号 like '*乙腈溶液*' or 规格型号 like '*in Acetonitrile*' or 英文名称 like '*in Acetonitrile*'),'[乙腈溶液]',
IIf(
(存货名称 like '*矿物油*' or 规格型号 like '*矿物油*' or 规格型号 like '*in mineral oil*'
or 英文名称 like '*in mineral oil*'),'[矿物油溶液]','NA')))as CAS索引号,* into ccc2 from NA
where
ID not in(select ID from ccc1)
and (存货名称 like '*盐酸溶液*'
and 存货名称 not like '*稀盐酸溶液*')
or 存货名称 like '*乙腈溶液*'
or 存货名称 like '*矿物油*'
or (规格型号 like '*盐酸溶液*' and 规格型号 not like '*稀盐酸溶液*')
or 规格型号 like '*乙腈溶液*'
or 规格型号 like '*矿物油*'
or 规格型号 like '*in Hydrochloric acid*'
or 规格型号 like '*in Acetonitrile*'
or 规格型号 like '*in mineral oil*'
or 英文名称 like '*in Hydrochloric acid*'
or 英文名称 like '*in Acetonitrile*'
or 英文名称 like '*in mineral oil*'

1