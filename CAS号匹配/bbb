SELECT
IIf(存货名称 like '*稀硝酸溶液*' or 规格型号 like '*稀硝酸溶液*' or 规格型号 like '*in dilute Nitric acid*'
or 英文名称 like '*in dilute Nitric acid*',CAS号&'[稀硝酸溶液]',
IIf(
((存货名称 like '*硝酸溶液*' and 存货名称 not like '*稀硝酸溶液*') or (规格型号 like '*硝酸溶液*' and 规格型号 not like '*稀硝酸溶液*')
or 规格型号 like '*in Nitric acid*' or 英文名称 like '*in Nitric acid*'),CAS号&'[硝酸溶液]',
IIf(
(存货名称 like '*稀盐酸溶液*' or 规格型号 like '*稀盐酸溶液*' or
规格型号 like '*in dilute hydrochloric acid*' or 英文名称 like '*in dilute hydrochloric acid*'),CAS号&'[稀盐酸溶液]',
IIf(
((存货名称 like '*盐酸溶液*' and 存货名称 not like '*稀盐酸溶液*') or 规格型号 like '*盐酸溶液*' and 规格型号 not like '*稀盐酸溶液*'
or 规格型号 like '*in Hydrochloric acid*' or 英文名称 like '*in Hydrochloric acid*'),CAS号&'[盐酸溶液]',
IIf(
(存货名称 like '*甲醛溶液*' or 规格型号 like '*甲醛溶液*' or 规格型号 like '*in Formaldehyde*' or 英文名称 like '*in Formaldehyde*'),CAS号&'[甲醛溶液]',
IIf(
(存货名称 like '*氯仿溶液*' or 规格型号 like '*氯仿溶液*'or 规格型号 like '*in Chloroform*' or 英文名称 like '*in Chloroform*'),CAS号&'[氯仿溶液]',
IIf(
(存货名称 like '*矿物油*' or 规格型号 like '*矿物油*' or 规格型号 like '*in mineral oil*' or 英文名称 like '*in mineral oil*'),CAS号&'[矿物油溶液]',
'NA'))))))) AS CAS索引号, * into bbb FROM noNA
where
(ID not in (select ID from aaa)
and 存货名称 like '稀硝酸溶液'
or (存货名称 like '*硝酸溶液*' and 存货名称 not like '*稀硝酸溶液*')
or 存货名称 like '*稀盐酸溶液*'
or (存货名称 like '*盐酸溶液*' and 存货名称 not like '*稀盐酸溶液*')
or 存货名称 like '*甲醛溶液*'
or 存货名称 like '*氯仿溶液*'
or 存货名称 like '*矿物油*'
or 规格型号 like '稀硝酸溶液'
or (规格型号 like '*硝酸溶液*' and 存货名称 not like '*稀硝酸溶液*')
or 规格型号 like '*稀盐酸溶液*'
or (规格型号 like '*盐酸溶液*' and 存货名称 not like '*稀盐酸溶液*')
or 规格型号 like '*甲醛溶液*'
or 规格型号 like '*氯仿溶液*'
or 规格型号 like '*矿物油*'
or 规格型号 like '*in dilute Nitric acid*'
or 规格型号 like '*in Nitric acid*'
or 规格型号 like '*in dilute hydrochloric acid*'
or 规格型号 like '*in Hydrochloric acid*'
or 规格型号 like '*in Formaldehyde*'
or 规格型号 like '*in Chloroform*'
or 规格型号 like '*in mineral oil*'
or 英文名称 like '*in dilute Nitric acid*'
or 英文名称 like '*in Nitric acid*'
or 英文名称 like '*in dilute hydrochloric acid*'
or 英文名称 like '*in Hydrochloric acid*'
or 英文名称 like '*in Formaldehyde*'
or 英文名称 like '*in Chloroform*'
or 英文名称 like '*in mineral oil*')


0