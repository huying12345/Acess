and 英文名称 not like '*in Nitric acid*'
and 英文名称 not like '*in dilute hydrochloric acid*'
and 英文名称 not like '*in Hydrochloric acid*'
and 英文名称 not like '*in Formaldehyde*'
and 英文名称 not like '*in Chloroform*'
and 英文名称 not like '*in mineral oil*')
or (规格型号 like '*solution*'
and 规格型号 not like '*aqueous solution*'
and 规格型号 not like '*in Methanol*'
and 规格型号 not like '*in Ethanol*'
and 规格型号 not like '*in Alcohol*'
and 规格型号 not like '*in Toluene*'
and 规格型号 not like '*in Carbon tetrachloride*'
and 规格型号 not like '*in Ether*'
and 规格型号 not like '*in Acetic acid*'
and 规格型号 not like '*in dilute Sulfuric acid*'
and 规格型号 not like '*in Sulfuric acid*'
and 规格型号 not like '*in dilute Nitric acid*'
and 规格型号 not like '*in Nitric acid*'
and 规格型号 not like '*in dilute hydrochloric acid*'
and 规格型号 not like '*in Hydrochloric acid*'
and 规格型号 not like '*in Formaldehyde*'
and 规格型号 not like '*in Chloroform*'
and 规格型号 not like '*in mineral oil*'),'手工检索','NULL')))as a,* from Sheet1
where CAS号 is not null and CAS号 <>'NULL' and CAS号 <>'NA' and 规格型号 not like '*HACH*'and 厂牌 not like '*HACH*'
and (英文名称 like '*Synthetic resin*'
or 规格型号 like '*Synthetic resin*'
or 英文名称 like '*Calconcarboxylic acid sodium salt*'
or 规格型号 like '*Calconcarboxylic acid sodium salt*'
or (英文名称 like '*solution*'
and 英文名称 not like '*aqueous solution*'
and 英文名称 not like '*in Methanol*'
and 英文名称 not like '*in Ethanol*'
and 英文名称 not like '*in Alcohol*'
and 英文名称 not like '*in Acetone*'
and 英文名称 not like '*in Toluene*'
and 英文名称 not like '*in Carbon tetrachloride*'
and 英文名称 not like '*in Ether*'
and 英文名称 not like '*in Acetic acid*'
and 英文名称 not like '*in dilute Sulfuric acid*'
and 英文名称 not like '*in Sulfuric acid*'
and 英文名称 not like '*in dilute Nitric acid*'
and 英文名称 not like '*in Nitric acid*'
and 英文名称 not like '*in dilute hydrochloric acid*'
and 英文名称 not like '*in Hydrochloric acid*'
and 英文名称 not like '*in Formaldehyde*'
and 英文名称 not like '*in Chloroform*'
and 英文名称 not like '*in mineral oil*')
or (规格型号 like '*solution*'
and 规格型号 not like '*aqueous solution*'
and 规格型号 not like '*in Methanol*'
and 规格型号 not like '*in Ethanol*'
and 规格型号 not like '*in Alcohol*'
and 规格型号 not like '*in Toluene*'
and 规格型号 not like '*in Carbon tetrachloride*'
and 规格型号 not like '*in Ether*'
and 规格型号 not like '*in Acetic acid*'
and 规格型号 not like '*in dilute Sulfuric acid*'
and 规格型号 not like '*in Sulfuric acid*'
and 规格型号 not like '*in dilute Nitric acid*'
and 规格型号 not like '*in Nitric acid*'
and 规格型号 not like '*in dilute hydrochloric acid*'
and 规格型号 not like '*in Hydrochloric acid*'
and 规格型号 not like '*in Formaldehyde*'
and 规格型号 not like '*in Chloroform*'
and 规格型号 not like '*in mineral oil*'))
)
union
(SELECT
IIf(
(存货名称 not like '*溶液*'
and 存货名称 not like '*矿物油*'
and 存货名称 not like '*合成树脂*'
and 存货名称 not like '*钙羧酸钠盐*')
and ((规格型号 not like '*solution*'
and 规格型号 not like '*aqueous solution*'
and 规格型号 not like '*in Methanol*'
and  规格型号 not like '*in Ethanol*'
and  规格型号 not like '*in Alcohol*'
and  规格型号 not like '*in Acetone*'
and 规格型号 not like '*in Toluene*'
and  规格型号 not like '*in Carbon tetrachloride*'
and  规格型号 not like '*in Ether*'
and  规格型号 not like '*in Acetic acid*'
and  规格型号 not like '*in dilute Sulfuric acid*'
and  规格型号 not like '*in Sulfuric acid*'
and  规格型号 not like '*in dilute Nitric acid*'
and  规格型号 not like '*in Nitric acid*'
and  规格型号 not like '*in dilute hydrochloric acid*'
and  规格型号 not like '*in Hydrochloric acid*'
and  规格型号 not like '*in Formaldehyde*'
and  规格型号 not like '*in Chloroform*'
and 规格型号 not like '*in mineral oil*'
and  规格型号 not like '*Synthetic resin*'
and  规格型号 not like '*Calconcarboxylic acid sodium salt*')
or (英文名称 not like '*solution*'
and 英文名称 not like '*aqueous solution*'
and 英文名称 not like '*in Methanol*'
and  英文名称 not like '*in Ethanol*'
and  英文名称 not like '*in Alcohol*'
and  英文名称 not like '*in Acetone*'
and 英文名称 not like '*in Toluene*'
and  英文名称 not like '*in Carbon tetrachloride*'
and  英文名称 not like '*in Ether*'
and  英文名称 not like '*in Acetic acid*'
and  英文名称 not like '*in dilute Sulfuric acid*'
and  英文名称 not like '*in Sulfuric acid*'
and  英文名称 not like '*in dilute Nitric acid*'
and  英文名称 not like '*in Nitric acid*'
and  英文名称 not like '*in dilute hydrochloric acid*'
and  英文名称 not like '*in Hydrochloric acid*'
and  英文名称 not like '*in Formaldehyde*'
and  英文名称 not like '*in Chloroform*'
and 英文名称 not like '*in mineral oil*'
and  英文名称 not like '*Synthetic resin*'
and  英文名称 not like '*Calconcarboxylic acid sodium salt*')),CAS号,'NULL') as a,* from Sheet1
where
(CAS号 is not null and CAS号 <>'NULL' and CAS号<>'NA' and 规格型号 not like '*HACH*' and 厂牌 not like '*HACH*'
and 存货名称 not like '*溶液*'
and 存货名称 not like '*矿物油*'
and 存货名称 not like '*合成树脂*'
and 存货名称 not like '*钙羧酸钠盐*')
and ((规格型号 not like '*solution*'
and 规格型号 not like '*aqueous solution*'
and 规格型号 not like '*in Methanol*'
and  规格型号 not like '*in Ethanol*'
and  规格型号 not like '*in Alcohol*'
and  规格型号 not like '*in Acetone*'
and 规格型号 not like '*in Toluene*'
and  规格型号 not like '*in Carbon tetrachloride*'
and  规格型号 not like '*in Ether*'
and  规格型号 not like '*in Acetic acid*'
and  规格型号 not like '*in dilute Sulfuric acid*'
and  规格型号 not like '*in Sulfuric acid*'
and  规格型号 not like '*in dilute Nitric acid*'
and  规格型号 not like '*in Nitric acid*'
and  规格型号 not like '*in dilute hydrochloric acid*'
and  规格型号 not like '*in Hydrochloric acid*'
and  规格型号 not like '*in Formaldehyde*'
and  规格型号 not like '*in Chloroform*'
and 规格型号 not like '*in mineral oil*'
and  规格型号 not like '*Synthetic resin*'
and  规格型号 not like '*Calconcarboxylic acid sodium salt*')
or (英文名称 not like '*solution*'
and 英文名称 not like '*aqueous solution*'
and 英文名称 not like '*in Methanol*'
and  英文名称 not like '*in Ethanol*'
and  英文名称 not like '*in Alcohol*'
and  英文名称 not like '*in Acetone*'
and 英文名称 not like '*in Toluene*'
and  英文名称 not like '*in Carbon tetrachloride*'
and  英文名称 not like '*in Ether*'
and  英文名称 not like '*in Acetic acid*'
and  英文名称 not like '*in dilute Sulfuric acid*'
and  英文名称 not like '*in Sulfuric acid*'
and  英文名称 not like '*in dilute Nitric acid*'
and  英文名称 not like '*in Nitric acid*'
and  英文名称 not like '*in dilute hydrochloric acid*'
and  英文名称 not like '*in Hydrochloric acid*'
and  英文名称 not like '*in Formaldehyde*'
and  英文名称 not like '*in Chloroform*'
and 英文名称 not like '*in mineral oil*'
and  英文名称 not like '*Synthetic resin*'
and  英文名称 not like '*Calconcarboxylic acid sodium salt*'))
)
union
(SELECT
IIf(
(存货名称 like '*甲醇溶液*' or 规格型号 like '*甲醇溶液*' or 规格型号 like '*in Methanol*'or 英文名称 like '*in Methanol*'),'[甲醇溶液]',
IIf(
(存货名称 like '*乙醇溶液*' or 规格型号 like '*乙醇溶液*' or 规格型号 like '*in Ethanol*' or 英文名称 like '*in Ethanol*'),'[乙醇溶液]',
IIf(
(存货名称 like '*丙酮溶液*' or 规格型号 like '*丙酮溶液*' or 规格型号 like '*in Acetone*' or 英文名称 like '*in Acetone*'),'[丙酮溶液]',
IIf(
(存货名称 like '*甲苯溶液*' or 规格型号 like '*甲苯溶液*' or 规格型号 like '*in Toluene*' or 英文名称 like '*in Toluene*'),'[甲苯溶液]',
IIf(
(存货名称 like '*四氯化碳溶液*' or 规格型号 like '*四氯化碳溶液*'or 规格型号 like '*in Carbon tetrachloride*'
or 英文名称 like '*in Carbon tetrachloride*'),'[四氯化碳溶液]',
IIf(
(存货名称 like '*乙醚溶液*' or 规格型号 like '*乙醚溶液*' or 规格型号 like '*in Ether*' or 英文名称 like '*in Ether*'),'[乙醚溶液]',
IIf(
(存货名称 like '*乙酸溶液*' or 规格型号 like '*乙酸溶液*'or 规格型号 like '*in Acetic acid*' or 英文名称 like '*in Acetic acid*'),'[乙酸溶液]',
IIf(
(存货名称 like '*稀硫酸溶液*' or 规格型号 like '*稀硫酸溶液*' or 规格型号 like '*in dilute Sulfuric acid*'
or 英文名称 like '*in dilute Sulfuric acid*'),'[稀硫酸溶液]',
IIf(
((存货名称 like '*硫酸溶液*' and 存货名称 not like '*稀硫酸溶液*') or (规格型号 like '*硫酸溶液*' and 规格型号 not like '*稀硫酸溶液*')
