SELECT  Sheet2.国家管控 as 管制品,
Sheet2.危险性类别 as 危险性质,
Sheet2.UN№ as UN,
newTable.*
FROM (SELECT a AS CAS索引号, * FROM
(select * from tableAA
union select * from tableBB
union select * from tableCC
union select * from tableDD
union select * from tableEE
union select * from tableFF))AS newTable
LEFT JOIN Sheet2
ON newTable.CAS索引号=Sheet2.CAS索引
ORDER BY newTable.ID;