update Sheet3
 set 管制品=switch(
            CAS索引号='手工确认','手工确认',
            CAS索引号='手工检索','手工检索',
            CAS索引号='NA','NULL',
),危险性质=switch(
            CAS索引号='手工确认','手工确认',
            CAS索引号='手工检索','手工检索',
            CAS索引号='NA','非危险品',
),UN=switch(
            CAS索引号='手工确认','手工确认',
            CAS索引号='手工检索','手工检索',
            CAS索引号='NA','NULL',
)
where  CAS索引号 ='手工确认' or CAS索引号 ='手工检索' or CAS索引号 ='NA'