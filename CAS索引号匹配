from Sheet1
right join newTable1
on Sheet1.CAS索引=newTable1.CAS索引号


insert into newTable1(管制品,危险性质,UN)
select 国家管控 from Sheet1,
select 危险性类别 from Sheet1,
select UN№ from Sheet1


---更新现有字段列
Update newTable1 INNER JOIN Sheet1
ON newTable1.CAS索引号=Sheet1.CAS索引
SET newTable1.管制品 = Sheet1.国家管控,
newTable1.危险性质=Sheet1.危险性类别,
newTable1.UN=Sheet1.UN№
WITH OWNERACCESS OPTION;


